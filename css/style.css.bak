body,html{
	height: 100%;
	width: 100%;
	font-family:calibri;
}

body,html,header,footer,section,div{
	padding:0;
	margin:0;
}

body{
	font-family:calibri;
	font-size: 100%;
	line-height: 24px;
	-webkit-font-smoothing:antized;
	background: #FFFFFF;
	color: #333333;
}
.inner p {
  text-align: justify;
}

a{
	color: #333333;
	text-decoration: none;
	outline: none;
}

a:hover{
	color: #333333;
	text-decoration: none;
}

img{
	vertical-align: middle;
	height: auto;
	max-width: 100%;
}
div:after,
div:before{
	content:"";
	display: block;
	clear: both;
}
p {
  font-size: 156%;
  font-weight: 500;
  line-height: 20px;
  margin: 0 0 10px;
}
.top-bar{
background:#3399cc;
text-align:right;
padding:3px 10px;
}
.main-header {
  background:url("../images/header.gif") no-repeat scroll center center/80%;
 /* height: 139px;*/
  background-color: #097a9f; 
  
  
}
.top-bar ul{
margin:0;
}
.top-bar ul li a {
  color: #fff;
  font-size: 144%;
  font-weight: 600;
}
nav {
  background:#0d4689;
  margin:1px 0 10px;
}
.top-main .news {
  min-height: 266px;
}
nav ul{
margin:0;
}
nav ul li a {
  color: #fff;
  display: inline-block;
  font-size: 164%;
  font-weight: 600;
  padding: 10px 17px;
  height:45px;
}
nav ul li a:hover{
color:#fff;
}
nav ul li {
  border-right: 1px solid #08557d;
  position:relative;
}
nav ul li ul {
  display: none;
  left: 0;
  position: absolute;
  top: 100%;
  width: 190px;
  z-index: 9999;
}
nav ul li ul li{
display:block;
border-bottom:1px solid #fff;
}
nav ul li ul li a{
width:100%;
}
nav ul li:hover ul {
  background: #0d4689 none repeat scroll 0 0;
  display: block;
  padding: 0;
}
.main-header{
text-align:center;
}
.logo{
padding:18px 0 0;
}
.emblem{
  padding: 21px 0 0;
}
.national_emblem{
 padding: 11px 0 0;
}
.col-sm-3.left-navigation{
  padding-right: 5px;
}
.col-md-6.banner {
  padding-left: 0;
}
.banner img {
  width: 100%;
}
.left-navigation ul .list-group-item {
  background-color: #3399cc;
  background-image: url("../images/left-arrow.png");
  background-position: 15px center;
  background-repeat: no-repeat;
  border-color:#0d4689;
  border-width:0 0 1px 0;
  margin: 0;
  border-radius:0;
}
.left-navigation ul .list-group-item a {
  color: #fff;
  font-size: 155%;
  font-weight: 600;
  padding-left: 18px;
}
.left-navigation ul li:last-child {
  background-image: none;
}
.main-content .top-main,.main-content .banner,.main-content .news ,.main-content .col-md-3,.body-container .main-content  {
  padding: 0 5px;
}
.news ul,.news {
  background: #1176aa none repeat scroll 0 0;
  list-style: outside none none;
  margin:0;
}
.news ul li a {
  color: #fff;
  font-size: 126%;
}
.news ul li{
	border-bottom:1px solid #00517a;
	padding:3px 0;
}
.news ul li p{
	color:#f6ff00;
}
.news h3 {
  /*background:url("../images/icon-stop.png") no-repeat scroll 80% center,url("../images/icon-play.png") no-repeat scroll 90% center;*/
  color: #fff;
  font-size: 16px;
  font-weight: bold;
  margin: 0 -5px;
  padding: 10px;
  background-color:#00517a;
}
.btnToggle{
  background: rgba(0, 0, 0, 0) url("../images/icon-play.png") no-repeat scroll 0 0;
  text-indent: -9999px;
  position: absolute;
  display: inline-block;
  height: 16px;
  width: 12px;
  right: 10px;

}
.btnToggle.et-run {
  background: rgba(0, 0, 0, 0) url("../images/icon-stop.png") no-repeat scroll right center;
  text-indent: -9999px;
  position: absolute;
  display: inline-block;
  height: 16px;
  width: 12px;
  right: 10px;
}
.main-button ul{
	list-style:none;
	background:#097a9f;
	margin:0;
}
.news p{
margin:0;
}

.main-button {
  background: #097a9f none repeat scroll 0 0;
  padding: 7px 6px;
}
.carousel-indicators {
  display: none;
}
.caption {
  background:#5a5e60;
  bottom: 0;
  color: #fff;
  margin: 0;
  opacity: 0.8;
  padding: 5px;
  position: absolute;
  width: 100%;
}
.main-button .orange{
background:#f36d21;
}
.main-button .blue{
background:#0d4689;
}
.main-button .yellow{
background:#ff9900;
}
.main-button .green{
background:#016331;
}
.main-button li a{
color:#fff;
}
.col-md-6.banner{
  padding-left: 0;
  position:relative;
}
.main-button ul li a {
  color: #fff;
  display: block;
  font-weight: 900;
  margin: 0 0 6px;
  padding: 18.5px 10px;
  font-size:141%;
}
.minister-profile{
background:#f6f6f0;
border:2px solid #097a9f;
border-radius:4px;
text-align:center;
}
.col-md-3.top-button{
padding-right:0;
}
.col-sm-3.minister-profile {
  padding: 16px 5px;
}
.news .view-all {
  color: #fff;
  display: block;
  text-align: right;
  font-size:144%;
}
.col-sm-9.main-content {
  padding-left:5px;
  position:relative;
}
.main-content h1 {
  background: #0074bc none repeat scroll 0 0;
  color: #fff;
  font-size: 18px;
  font-weight: bold;
  padding: 10px;
  margin:4px 0 10px;
}
.bottom-slider img {
  border: 2px solid #0090bf;
  border-radius: 3px;
}
.bottom-slider ul {
  padding-top: 15px;
  text-align:center;
}
.bottom-slider h3 {
  border-bottom: 2px solid #0090bf;
  color: #0090bf;
  display: block;
  margin: 0 0 8px;
  padding: 0 0 6px;
  position: absolute;
  top: -37px;
  width: 74.4%;
}
.bottom-slider ul li {
  padding-left: 0;
}
.bottom-slider.col-md-12 {
  margin-top: 0;
  padding: 0 10px;
}
.bottom-main {
  padding-bottom: 30px;
}
.inner th {
  font-size: 173%;
  text-align: center;
}
.inner table td {
  padding: 16px 12px !important;
}
table{
  font-size: 156%;
}
.breadcrumb {
  font-size: 156%;
}

footer{
background:#3399cc;
border-top:10px solid #0074bc;
color: #fff;
text-align:center;
padding:20px 0 0;
}
footer li a,footer p {
  color: #fff;
  font-size:132%;
  font-weight: bold;
}
footer li {
  border-left: 2px solid;
  line-height: 13px;
}
footer li:first-child{
border:none;
} 
.modern-ticker {
  font-size: 16px;
  line-height: 30px;
}
.modern-ticker {
  float: left;
  height: 107px;
  margin-bottom: 13px;
  margin-left: 5px;
  margin-top: 13px;
  overflow: hidden;
  padding: 2px 11px 2px 2px;
  position: relative;
  width: 98%;
}
.mt-news {
  float: left;
  margin-left: 25px;
  overflow: hidden;
  width: 96% !important;
}
.mt-controls {
  background-position: -1000px center;
  background-repeat: no-repeat;
  height: 100%;
  margin-left: 0;
  right: 2px;
  width: 100%;
  z-index: -1;
}
.mt-news ul {
  background-image: url("images/bullet-over.png");
}
.modern-ticker ul {
  list-style: outside none none;
  padding: 0;
}
.mt-news li {
  float: left;
  padding-left: 5px;
}
.modern-ticker ul li {
  background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
  list-style: outside none none;
}
.mt-next {
  background: rgba(0, 0, 0, 0) url("../images/right_slide.png") no-repeat scroll left top;
  cursor: pointer;
  height: 25px;
  position: absolute;
  right: 1px;
  top: 38px;
  width: 16px;
  z-index: 1;
}
.mt-prev {
  background: rgba(0, 0, 0, 0) url("../images/left_slide.png") no-repeat scroll left top;
  cursor: pointer;
  height: 25px;
  left: 0;
  margin-right: 20px;
  position: absolute;
  top: 38px;
  width: 16px;
  z-index: 1;
}
.org{
  text-align:center;
}
.mobile-nav{
	display:none;
}


